{

  "autoassign": {
    "path": "./src/plugins/autoassign",
    "dependencies": [
      "events",
      "logger",
      "review",
      "pull-request-review"
    ]
  },

  "badges-review": {
    "path": "./src/plugins/badges/badges-review",
    "options": {
      "url": "http://localhost/badges/"
    },
    "dependencies": [
      "queue",
      "events",
      "logger",
      "pull-request-github"
    ]
  },

  "reminder": {
    "path": "./src/plugins/reminder",
    "options": {
      "days": 1
    },
    "dependencies": [
      "model",
      "events",
      "logger"
    ]
  },

  "yandex-startrek": {
    "path": "./src/plugins/yandex-startrek",
    "options": {
      "url": "https://st-api.yandex-team.ru/v2"
    }
  },

  "#include:startrek": "./default-startrek.json",

  "yandex-staff": {
    "path": "./src/plugins/yandex-staff",
    "options": {
      "cache": {
        "getGroupMembers": 86400
      },
      "center_url": "https://center.yandex-team.ru/api/v1/",
      "jabber_url": "https://center.yandex-team.ru/jabber/status-bulk/"
    }
  }

}
