{

  "team-search": {
    "options": {
      "defaultDriver": "yandex-staff"
    }
  },

  "team-search-github": {
    "path": "./src/services/team-search/search-github",
    "dependencies": [
      "github"
    ],
    "#plugin:team-search": {
      "drivers": {
        "github": "team-search-github"
      }
    }
  },

  "team-search-yandex-staff": {
    "path": "./src/services/team-search/search-yandex-staff",
    "dependencies": [
      "yandex-staff"
    ],
    "#plugin:team-search": {
      "drivers": {
        "yandex-staff": "team-search-yandex-staff"
      }
    }
  },

  "team-driver-static": {
    "path": "./src/services/team-sync/driver-static",
    "dependencies": [
      "model"
    ],
    "#plugin:team-sync": {
      "drivers": {
        "static": "team-driver-static"
      }
    }
  },

  "team-driver-github": {
    "path": "./src/services/team-sync/driver-github",
    "dependencies": [
      "github"
    ],
    "#plugin:team-sync": {
      "drivers": {
        "github": "team-driver-github"
      }
    }
  },

  "team-driver-yandex-staff": {
    "path": "./src/services/team-sync/driver-yandex-staff",
    "dependencies": [
      "yandex-staff"
    ],
    "#plugin:team-sync": {
      "drivers": {
        "yandex-staff": "team-driver-yandex-staff"
      }
    }
  }

}
