{

  "http-routes-badges": {
    "path": "./src/plugins/badges-http",
    "options": {
      "template": "flat-square"
    },
    "#plugin:http": {
      "routes": {
        "/badges": "http-routes-badges"
      }
    }
  },

  "http-routes-user": {
    "path": "./src/services/model/model-user/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/users": "http-routes-user"
      }
    }
  },

  "http-routes-team": {
    "path": "./src/services/model/model-team/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/teams": "http-routes-team"
      }
    }
  },

  "http-routes-team-manager": {
    "path": "./src/services/team-manager/routes",
    "dependencies": [
      "logger",
      "team-manager"
    ],
    "#plugin:http": {
      "routes": {
        "/team-manager": "http-routes-team-manager"
      }
    }
  },

  "#comment:http-routes-github": {
    "path": "./src/services/pull-request-webhook",
    "dependencies": [
      "events",
      "logger",
      "pull-request-model",
      "pull-request-github"
    ],
    "#plugin:http": {
      "routes": {
        "/github": "http-routes-github"
      }
    }
  }

}
