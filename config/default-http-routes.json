{

  "http-middleware-cors-headers": {
    "path": "./src/services/http/middlewares/cors-headers",
    "#plugin:http": {
      "middlewares": ["http-middleware-cors-headers"]
    }
  },

  "http-middleware-handle-error": {
    "path": "./src/services/http/middlewares/handle-error",
    "#plugin:http": {
      "middlewares": ["http-middleware-handle-error"]
    }
  },

  "http-routes-badges": {
    "path": "./src/plugins/badges-http",
    "options": {
      "template": "flat-square"
    },
    "#plugin:http": {
      "routes": {
        "/badges": "http-routes-badges"
      }
    }
  },

  "http-routes-user": {
    "path": "./src/services/model/model-user/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/users": "http-routes-user"
      }
    }
  },

  "http-routes-team": {
    "path": "./src/services/model/model-team/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/teams": "http-routes-team"
      }
    }
  },

  "http-routes-pull-request": {
    "path": "./src/services/model/model-pull-request/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/pulls": ["http-routes-pull-request"]
      }
    }
  },

  "http-routes-pull-request-review": {
    "path": "./src/services/pull-request-review/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/pulls": ["http-routes-pull-request-review"]
      }
    }
  },

  "http-routes-pull-request-badges": {
    "path": "./src/plugins/badges/routes",
    "dependencies": [
      "model",
      "events",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/pulls": ["http-routes-pull-request-badges"]
      }
    }
  },

  "http-routes-team-manager": {
    "path": "./src/services/team-manager/routes",
    "dependencies": [
      "logger",
      "team-manager"
    ],
    "#plugin:http": {
      "routes": {
        "/team-manager": "http-routes-team-manager"
      }
    }
  },

  "http-routes-review": {
    "path": "./src/services/review/routes",
    "dependencies": [
      "logger",
      "review"
    ],
    "#plugin:http": {
      "routes": {
        "/review": "http-routes-review"
      }
    }
  },

  "http-routes-webhook": {
    "path": "./src/services/webhook",
    "dependencies": [
      "model",
      "events",
      "logger",
      "pull-request-github"
    ],
    "#plugin:http": {
      "routes": {
        "/webhook": "http-routes-webhook"
      }
    }
  }

}
