{

  "http-middleware-model": {
    "path": "./src/services/model/response",
    "#plugin:http": {
      "middlewares": [
        "http-middleware-model"
      ]
    }
  },

  "http-routes-user": {
    "path": "./src/services/model/model-user/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/users": "http-routes-user"
      }
    }
  },

  "http-routes-team": {
    "path": "./src/services/model/model-team/routes",
    "dependencies": [
      "model",
      "logger"
    ],
    "#plugin:http": {
      "routes": {
        "/teams": "http-routes-team"
      }
    }
  }

}
